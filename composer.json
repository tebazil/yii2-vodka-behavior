{
    "name": "tebazil/yii2-vodka-behavior",
    "description": "Yii2 joke extension that intercepts the application response and replaces it with joke messages",
    "type": "yii2-extension",
    "authors": [
        {
            "name": "Vasiliy Baukin",
            "email": "vasiliy.baukin@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/tebazil/yii2-vodka-behavior/issues?state=open",
        "source": "https://github.com/tebazil/yii2-vodka-behavior"
    },

    "require": {
        "yiisoft/yii2": "^2.0"
    },

    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },

    "autoload": {
        "psr-4": {
            "tebazil\\yii2\\extensions\\vodka\\": "src"
        }
    }

}